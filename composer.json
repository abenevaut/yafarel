{
  "name": "abenevaut/yaf-framework",
  "type": "project",
  "description": "Yet another YAF framework",
  "keywords": [
    "YAF",
    "PHP"
  ],
  "license": "GPL-3.0-only",
  "require": {
    "php": "^8.1",
    "ext-yaf": "^3.1",
    "illuminate/console": "^10.7",
    "illuminate/database": "^10.7",
    "illuminate/events": "^10.7",
    "monolog/monolog": "^3.3",
    "nesbot/carbon": "^2.66"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Console\\": "app/Console/",
      "App\\Domain\\": "app/Domain/",
      "App\\Services\\": "app/Services/",
      "Tests\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
